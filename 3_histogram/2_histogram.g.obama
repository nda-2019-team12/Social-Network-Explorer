# it is a two month period from 2016/02/23 till 2016/04/23.
dt.r.obama.subset <- subset(dt.reddit.master, date > "2016/02/23" & date < "2016/04/23")

#histogram
ggplot(dt.r.obama.subset, aes(x = TIMESTAMP)) + geom_histogram() + 
  scale_x_datetime(date_labels = "%m/%d", breaks = "1 day") + ggtitle("Obama's visit to Cuba") + 
  labs(y = "Count", x = "Date") + theme(axis.text.x = element_text(size = 7, angle = 90))
